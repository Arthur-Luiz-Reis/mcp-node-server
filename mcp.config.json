{
  "type": "nodejs",
  "mainFile": "build/index.js",
  "version": "18",
  "env": {
    "NODE_ENV": "production"
  },
  "docker": {
    "baseImage": "node:18-slim",
    "ports": [3000],
    "volumes": [],
    "command": "node build/index.js"
  },
  "healthCheck": {
    "path": "/health",
    "interval": "30s",
    "timeout": "10s",
    "retries": 3
  },
  "resources": {
    "memory": "512M",
    "cpus": 1
  }
}
